    <h1> Edit a Post </h1>

    <%= form_for @blogpost do |f| %>
        Title: <%= f.text_field :title %><br><br>
        Camera: <%= f.text_field :camera %><br><br>
        Description: <%= f.text_field :desc %><br><br>
        

        <%= f.submit "Save Changes" %>

    <% end %><br>

    <%= link_to "Back to the list!", blogposts_path %>